{
  "name": "cephee/laravel-mondialrelay",
  "description": "Client Laravel pour l'API Mondial Relay",
  "keywords": ["laravel", "laravel5", "mondial relay", "api"],
  "require": {
    "illuminate/support": "^6.0|^7.0|^8.0",
    "quentinbontemps/php-mondialrelay-api": "^1.0"
  },
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Body Joffrey",
      "email": "bodyjoffrey@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Cephee\\LaravelMondialRelay\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Cephee\\LaravelMondialRelay\\LaravelMondialRelayServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
